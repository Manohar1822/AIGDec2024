<!--<%- include('partials/header') %>
<%- include('partials/navbar') %> 
<div class="container mt-5">
  <h1>Register Student</h1>

  <div class="row">
    <div class="col-sm-8">
      <div class="card">
        <div class="card-body">

           Makes POST request to /register route 
          <form action="/registerstudent" method="POST">
            <div class="form-group">
                <label for="email">*Email</label>
                <p style="color:red;"><%= message %> </p>
                <input type="email" required class="form-control" name="username">
              </div>
            <div class="form-group">
                <label for="password">*Set Password</label>
                <input type="password" required class="form-control" name="password">
              </div>
              <div class="form-group">
                <label for="phone">*Your Phone Number</label>
                <input type="tel" required class="form-control" name="phone">
              </div>
              <div class="form-group">
                <label for="optionalphone">Emergency/Parents Phone Number</label>
                <input type="tel" required class="form-control" name="optionalPhone">
              </div>
              <div class="form-group">
                <label for="typeOfID">*Type of Unique ID used in your Organization</label>
                <select name="typeOfID" >
                    <option value="PRN">PRN</option>
                    <option value="RollNumber">Roll Number</option>
                    <option value="Other">Other</option>
                </select>
              </div>
              <div class="form-group">
                <label for="IDOfOrg">Enter your Chosen ID</label>
                <input type="text" required class="form-control" name="IDOfOrg">
              </div>

              <div class="form-group">
                <label for="class">Choose your class</label>
                <select name="class" required>
                    <%if(classes!=false){%>
                        <%classes.forEach(function(clas){%>
  
                          <option value="<%=clas._id%>"><%=clas.className%></option>
                        <%})%>
                      <%}%>
                </select>
              </div>

            <button type="submit" class="btn btn-dark">Register</button>
          </form>

        </div>
        <a href="/loginstudent" class="btn btn-light btn-lg" >Already User?, Login</a>
      </div>
    </div>

